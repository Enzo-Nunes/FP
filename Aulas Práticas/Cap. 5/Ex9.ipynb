   "source": [
    "def num_occ_lista(l, n):\n",
    "    count = 0\n",
    "    while l != []:\n",
    "        first = l[0]\n",
    "        l = l[1:]\n",
    "        if isinstance(first, list):\n",
    "            l += first\n",
    "        elif first == n:\n",
    "            count += 1\n",
    "    return count\n"
   ]
  }
