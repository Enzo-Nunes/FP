   "source": [
    "x = eval(input(\"Escreva um número:\"))\n",
    "s = 0\n",
    "\n",
    "while x != 0:\n",
    "    s += x % 10\n",
    "    x = x // 10\n",
    "    s *= 10\n",
    "\n",
    "print(\"O número invertido é:\", s//10)"
   ]
