   "source": [
    "soma = 1\n",
    "i = 1\n",
    "termo = 1\n",
    "\n",
    "x = eval(input(\"Escreva um valor para x:\"))\n",
    "n = eval(input(\"Escreva um valor para n:\"))\n",
    "\n",
    "while i <= n:\n",
    "    termo = termo * (x/i)\n",
    "    soma = soma + termo\n",
    "    i = i + 1\n",
    "print(soma)"
   ]
  }
