   "source": [
    "def pertence(w, n):\n",
    "    if w == []:\n",
    "        return False\n",
    "    if w[0] == n:\n",
    "        return True\n",
    "    return pertence(w[1:], n)\n",
    "\n",
    "def subtrai(w1, w2):\n",
    "    if w1 == []:\n",
    "        return []\n",
    "    if pertence(w1[0], w2):\n",
    "        return subtrai(w1[1:], w2)\n",
    "    return [w1[0]] + subtrai(w1[1:], w2)"
   ]
  }
