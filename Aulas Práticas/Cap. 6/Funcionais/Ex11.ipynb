   "source": [
    "def Fixed_Point(f, x):\n",
    "    while x != f(x):\n",
    "        x = f(x)\n",
    "    return x\n",
    "\n",
    "def num_dig(n):\n",
    "    return Fixed_Point(lambda x: x if x[0] == 0 else (x[0]//10, x[1] + 1), (n, 0))[1]"
   ]
  }
