   "source": [
    "def piatorio(l_inf, l_sup, calc_termo, prox):\n",
    "    res = 1\n",
    "    while l_inf <= l_sup:\n",
    "        res    *= calc_termo(l_inf)\n",
    "        l_inf   = prox(l_inf)\n",
    "    return res\n",
    "\n",
    "def factorial(n):\n",
    "    return piatorio(1, n, lambda x: x, lambda x: x + 1)\n",
    "\n"
   ]
  }
