   "source": [
    "def junta_ordenadas(l1, l2):\n",
    "    if l1 == []:\n",
    "        return l2\n",
    "    if l2 == []:\n",
    "        return l1\n",
    "    if l1[0] < l2[0]:\n",
    "        return [l1[0]] + junta_ordenadas(l1[1:], l2)\n",
    "    return [l2[0]] + junta_ordenadas(l1, l2[1:])"
   ]
  }
