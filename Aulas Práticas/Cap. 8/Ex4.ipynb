   "source": [
    "def resumo_FP(notas):\n",
    "    apr, rpv, soma = 0, 0, 0\n",
    "    for k, v in notas.items():\n",
    "        if k < 10:\n",
    "            rpv += len(v)\n",
    "        else:\n",
    "            apr += len(v)\n",
    "            soma += k * len(v)\n",
    "    return soma/apr, rpv"
   ]
  }
