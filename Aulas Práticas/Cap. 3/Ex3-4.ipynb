    "def area_circulo(r):\n",
    "    return 3.14 * r ** 2\n",
    "\n",
    "def area_coroa(r_1, r_2):\n",
    "    if r_1 > r_2:\n",
    "        raise ValueError(\"r1 deve ser menor que r2\")\n",
    "    \n",
    "    coroa = area_circulo(r_2) - area_circulo(r_1)\n",
    "\n",
    "    return coroa\n",
    "print(area_coroa(2, 4))"
   ]
  }
